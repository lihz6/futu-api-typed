{
  "name": "futu-sdk",
  "version": "0.0.9",
  "private": false,
  "type": "module",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    },
    "./proto": {
      "require": "./dist/proto.cjs",
      "import": "./dist/proto.js"
    }
  },
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lihz6/futu-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/lihz6/futu-sdk/issues"
  },
  "homepage": "https://github.com/lihz6/futu-sdk",
  "tsup": {
    "entry": {
      "proto": "./proto/index.js",
      "index": "./src/index.ts"
    },
    "format": [
      "cjs",
      "esm"
    ],
    "splitting": false,
    "minify": true,
    "clean": true,
    "dts": true
  },
  "scripts": {
    "proto": "cp ./node_modules/futu-api/proto/*.proto proto/files && pbjs --force-long -t static-module -w es6 proto/files/*.proto | tee proto/index.js | pbts -o proto/index.d.ts -",
    "build": "tsup --external futu-sdk/proto"
  },
  "keywords": [
    "futu",
    "futu-api",
    "futu-open-api"
  ],
  "files": [
    "dist",
    "src",
    "proto/files",
    "proto/index.d.ts",
    "LICENSE",
    "package.json",
    "package-lock.json"
  ],
  "author": "Li Hongzhang",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@types/long": "^5.0.0",
    "bufferutil": "^4.0.8",
    "long": "^5.2.3",
    "protobufjs": "^7.4.0",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@types/ws": "^8.5.12",
    "futu-api": "^8.5.4508",
    "protobufjs-cli": "^1.1.3",
    "tsup": "^8.2.4",
    "typescript": "^5.5.4"
  }
}
