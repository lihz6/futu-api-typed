{
  "name": "futu-sdk",
  "version": "0.1.1",
  "private": false,
  "type": "module",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    },
    "./proto": {
      "require": "./dist/proto.cjs",
      "import": "./dist/proto.js"
    }
  },
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lihz6/futu-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/lihz6/futu-sdk/issues"
  },
  "homepage": "https://github.com/lihz6/futu-sdk",
  "tsup": {
    "format": [
      "esm",
      "cjs"
    ],
    "splitting": false,
    "minify": true,
    "clean": true,
    "dts": true
  },
  "scripts": {
    "proto": "pbjs -t static-module -w es6 node_modules/futu-api/proto/*.proto | tee node_modules/futu-sdk/dist/proto.js | pbts -o node_modules/futu-sdk/dist/proto.d.ts -",
    "build": "tsup --external futu-sdk/proto --entry.index src/index.ts --entry.proto node_modules/futu-sdk/dist/proto.js",
    "dist": "npm run proto && npm run build && rsync -a node_modules/futu-api/proto/*.proto dist/files"
  },
  "keywords": [
    "futu",
    "futu-api",
    "futu-open-api"
  ],
  "files": [
    "dist",
    "LICENSE",
    "package.json",
    "package-lock.json"
  ],
  "author": "Li Hongzhang",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@types/long": "^5.0.0",
    "@types/ws": "^8.5.12",
    "bufferutil": "^4.0.8",
    "long": "^5.2.3",
    "protobufjs": "^7.4.0",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "futu-api": "^8.5.4508",
    "futu-sdk": "^0.1.1",
    "protobufjs-cli": "^1.1.3",
    "tsup": "^8.2.4",
    "typescript": "^5.5.4"
  }
}
